{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sample-extenstion-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sampleExtenstion/SampleExtenstionWebPart.js",
          "manifest": "./src/webparts/sampleExtenstion/SampleExtenstionWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.5.1.js",
      "globalName": "jQuery"
    },
    "datatables.net": {
      "path": "https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js",
      "globalName": "jQueryData"
      
    },
    
    "jqueryTableBoot":{
      "path": "https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap4.min.js",
      "globalName": "jQueryTable"     
    }
    ,
    "jqueryButton":{
      "path": "https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js",
      "globalName": "jqueryButtonJS"
    }
    ,
    "jqueryJSZip":{
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js",
      "globalName": "jqueryJSZipJS"
    }
    ,
    "jqueryPdf":{
      "path": "https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js",
      "globalName": "jqueryPdfJS"
    }
    ,
    "jqueryFont":{
      "path": "https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js",
      "globalName": "jqueryFontJS"
    }
    ,
    "jqueryHtml5":{
      "path": "https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js",
      "globalName": "jqueryHtml5JS"
    },
    "jqueryCEvent":{
      "path": "https://www.cvent.com/g/mobile/javascript/calendar-widget-loader.js?version=469888.0111436111",
      "globalName": "JqueryCEventJS"
    }
  },
  
  "localizedResources": {
    "SampleExtenstionWebPartStrings": "lib/webparts/sampleExtenstion/loc/{locale}.js"
  }
}
